{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gamekult",
  "title": "Gamekult",
  "description": "Read the latest news and search from video games.",
  "icon": "icon.png",
  "author": "Cronos87",
  "categories": [
    "News",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "news",
      "title": "Show Latest News",
      "subtitle": "Gamekult",
      "description": "Search the latest news from Gamekult.",
      "mode": "view"
    },
    {
      "name": "search-games",
      "title": "Search Games",
      "subtitle": "Gamekult",
      "description": "Search and open game's records.",
      "mode": "view"
    },
    {
      "name": "search-articles",
      "title": "Search Articles",
      "subtitle": "Gamekult",
      "description": "Search and open articles.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "node-fetch": "^3.2.10",
    "node-html-parser": "5.4.2-0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "~18.7.3",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "react-devtools": "^4.25.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
